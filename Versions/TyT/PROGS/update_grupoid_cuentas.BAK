SELECT A.ID AS CID, B.ID AS CGrupoID FROM CUENTAS A ;
	INNER JOIN CUENTAS B ON (A.CtaSup = B.Código) WHERE A.CtaSup != "0" ;
	INTO CURSOR SQLTEMP ORDER BY A.ID

	
SELECT SQLTEMP
SCAN ALL 
	SELECT CUENTAS
	REPLACE CUENTAS.GrupoID WITH SQLTEMP.CGrupoID FOR CUENTAS.ID = SQLTEMP.CID

	SELECT SQLTEMP
ENDSCAN	
	