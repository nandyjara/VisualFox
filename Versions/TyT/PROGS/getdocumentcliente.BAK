PARAMETERS lcTipo, lcDocumentoID
#INCLUDE DOBRA.H

lcTipo = STRTRAN(lcTipo,'*')
 
MESSAGEBOX(lcTipo)
MESSAGEBOX(lcDocumentoID)

DO CASE
	CASE lcTipo = IDDOC_CLI_INGRESO
		DO FORM CLI_DOCUMENT_INGRESOS WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_DEBITO
		DO FORM CLI_DOCUMENT_DEBITOS WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_CREDITO			
		DO FORM CLI_DOCUMENT_CREDITOS WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_CHEQUES			
		DO FORM CLI_DOCUMENT_CHEQUES WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_INGRESO_CHEQUE	
		DO FORM CLI_DOCUMENT_INGRESOS WITH lcDocumentoID

	CASE lcTipo = IDDOC_CLI_INGRESO_MASIVO	
		DO FORM CLI_DOCUMENT_INGRESOS_MASIVOS WITH lcDocumentoID

	CASE lcTipo = IDDOC_CLI_INGRESO_COBRANZAS	
		DO FORM CLI_DOCUMENT_COBRANZAS WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_LETRAS_CAMBIO		
		DO FORM CLI_DOCUMENT_LETRAS WITH lcDocumentoID

	CASE INLIST(lcTipo,IDDOC_VEN_FACTURAS,IDDOC_VEN_FACTURAS_LETRAS)
		DO FORM VEN_DOCUMENT_FACTURAS WITH lcDocumentoID

	CASE lcTipo = IDDOC_VEN_DEVOLUCIONES
		DO FORM VEN_DOCUMENT_DEVOLUCIONES WITH lcDocumentoID	

	CASE lcTipo = IDDOC_CLI_RETENCION
		DO FORM CLI_DOCUMENT_RETENCIONES WITH lcDocumentoID	
************************************************************
	CASE lcTipo = IDDOC_EMP_DEBITO_MASIVO
		DO FORM EMP_DOCUMENT_DEBITO_MASIVO WITH lcDocumentoID	

	CASE lcTipo = IDDOC_EMP_DEBITO_COMIDA
		DO FORM EMP_DOCUMENT_DEBITO_COMIDA WITH lcDocumentoID	

	CASE lcTipo = IDDOC_EMP_ROL
		DO FORM EMP_DOCUMENT_ROLES WITH lcDocumentoID

	CASE lcTipo = IDDOC_EMP_PRESTAMO_MASIVO
		DO FORM EMP_DOCUMENT_PRESTAMOS_MASIVOS WITH lcDocumentoID	

	CASE lcTipo = IDDOC_EMP_PRESTAMO
		DO FORM EMP_DOCUMENT_PRESTAMO WITH lcDocumentoID	

************************************************************
	OTHERWISE
		MESSAGEBOX("Lo siento, pero no se puede presentar el documento.")		
ENDCASE