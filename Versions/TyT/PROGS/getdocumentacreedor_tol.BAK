PARAMETERS lcTipo, lcDocumentoID
#INCLUDE DOBRA.H

lcTipo = STRTRAN(lcTipo,'*')

DO CASE
	CASE lcTipo = IDDOC_IMP_PEDIDOS
		DO FORM IMP_DOCUMENT_PEDIDO WITH lcDocumentoID

	CASE lcTipo = IDDOC_ACR_EGRESO	
		DO FORM ACR_DOCUMENT_EGRESO WITH lcDocumentoID	

	CASE lcTipo = IDDOC_ACR_DEBITO			
		DO FORM ACR_DOCUMENT_DEBITO WITH lcDocumentoID	

	CASE lcTipo = IDDOC_ACR_CREDITO
		DO FORM ACR_DOCUMENT_CREDITO WITH lcDocumentoID	

	CASE lcTipo = IDDOC_ACR_RETENCION
		DO FORM ACR_DOCUMENT_RETENCIONES WITH lcDocumentoID	

	CASE lcTipo = IDDOC_COM_FACTURAS
		DO FORM COM_DOCUMENT_FACTURAS WITH lcDocumentoID

	CASE lcTipo = IDDOC_COM_DEVOLUCIONES
		DO FORM COM_DOCUMENT_DEVOLUCIONES WITH lcDocumentoID

	OTHERWISE
		MESSAGEBOX("Lo siento, pero no se puede presentar el documento.")
ENDCASE